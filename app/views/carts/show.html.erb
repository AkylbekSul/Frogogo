<div class="flex flex-col w-full justify-between h-full gap-3">
  <div class="flex flex-row justify-between items-center w-full">
    <h1 class="font-bold">Корзина</h1>
    <% if @current_cart.cart_items.any? %>
      <%= button_to 'Очистить корзину', reset_cart_path, method: :delete, class: "font-thin", data: { turbo_frame: "cart_content", confirm: "Вы уверены, что хотите очистить корзину?" } %>
    <% end %>
  </div>

  <%= turbo_frame_tag 'cart_content' do %>
    <% if @current_cart.cart_items.any? %>
      <div class="flex flex-col">
        <% @current_cart.cart_items.each do |cart_item| %>
          <div id="cart_item_<%= cart_item.id %>" class="flex flex-row justify-between items-center">
            <div class="basis-1/4 flex flex-row items-center">
              <%= image_tag cart_item.product.image, class: "w-20 h-20 object-cover" %>
              <div>
                <p><%= cart_item.product.description %></p>
              </div>
            </div>
            <div class="basis-1/2 flex justify-center">
              <div class="flex items-center space-x-4 p-2 bg-gray-100 rounded-lg ">
                <%= button_to decrement_cart_item_path(cart_item), method: :post, turbo_stream: true, class: "p-2 mb-1" do %>
                  <svg width="8" height="2" viewBox="0 0 8 2" fill="none" xmlns="http://www.w3.org/2000/svg">
                    <path fill-rule="evenodd" clip-rule="evenodd" d="M7.33333 0.333328C7.70152 0.333328 8 0.631805 8 0.999995C8 1.36818 7.70152 1.66666 7.33333 1.66666H0.666667C0.298477 1.66666 0 1.36818 0 0.999995C0 0.631805 0.298477 0.333328 0.666667 0.333328H7.33333Z" fill="#1E1E3C"/>
                  </svg>
                <% end %>

                <p id="cart_item_quantity<%= cart_item.id %>" class="text-lg font-medium text-gray-800">
                  <%= cart_item.quantity %>
                </p>

                <%= button_to increment_cart_item_path(cart_item), method: :post, turbo_stream: true, class: "p-2" do %>
                  <svg width="8" height="8" viewBox="0 0 8 8" fill="none" xmlns="http://www.w3.org/2000/svg">
                    <path fill-rule="evenodd" clip-rule="evenodd" d="M4.57024 3.45161H7.44355C7.75087 3.45161 8 3.70074 8 4.00806C8 4.31538 7.75087 4.56452 7.44355 4.56452H4.57024V7.42144C4.57024 7.74097 4.31121 8 3.99168 8C3.67214 8 3.41311 7.74097 3.41311 7.42144V4.56452H0.556452C0.249132 4.56452 0 4.31538 0 4.00806C0 3.70074 0.249132 3.45161 0.556452 3.45161H3.41311V0.578564C3.41311 0.259032 3.67214 0 3.99168 0C4.31121 0 4.57024 0.259032 4.57024 0.578564V3.45161Z" fill="#1E1E3C"/>
                  </svg>
                <% end %>
              </div>
            </div>
            <div class="basis-1/4 flex flex-col items-center">
              <%= button_to cart_item_path(cart_item), method: :delete, class: "font-thin" do %>
                <svg width="20" height="20" viewBox="0 0 20 20" fill="none" xmlns="http://www.w3.org/2000/svg">
                  <path fill-rule="evenodd" clip-rule="evenodd" d="M13.6918 19.1667C14.9264 19.1667 15.72 18.4102 15.7761 17.1576L16.3213 5.437H17.2031C17.604 5.437 17.9166 5.11978 17.9166 4.7131C17.9166 4.31455 17.604 3.99733 17.2031 3.99733H13.6437V2.89115C13.6437 1.63856 12.7939 0.833328 11.423 0.833328H8.56095C7.19808 0.833328 6.34829 1.63856 6.34829 2.89115V3.99733H2.79682C2.40399 3.99733 2.08331 4.31455 2.08331 4.7131C2.08331 5.11978 2.40399 5.437 2.79682 5.437H3.68669L4.22382 17.1576C4.28795 18.4184 5.07361 19.1667 6.32424 19.1667H13.6918ZM12.1445 3.99733H7.85547V2.96435C7.86349 2.517 8.18416 2.19979 8.65716 2.19979H11.3348C11.8078 2.19979 12.1365 2.517 12.1365 2.96435L12.1445 3.99733ZM13.5154 17.7107H6.48461C6.04368 17.7107 5.72301 17.3773 5.69896 16.8974L5.16182 5.437H14.8222L14.3091 16.8974C14.285 17.3854 13.9724 17.7107 13.5154 17.7107ZM12.369 16.3768C12.6656 16.3768 12.8821 16.1247 12.8981 15.783L13.1386 7.44602C13.1466 7.1044 12.9142 6.84412 12.6175 6.84412C12.3289 6.84412 12.0884 7.11254 12.0804 7.43788L11.8399 15.7668C11.8319 16.1084 12.0563 16.3768 12.369 16.3768ZM7.63901 16.3768C7.94365 16.3768 8.17614 16.1084 8.16812 15.7668L7.92762 7.43788C7.91158 7.11254 7.67108 6.84412 7.39049 6.84412C7.08585 6.84412 6.86137 7.1044 6.86939 7.44602L7.1099 15.783C7.11791 16.1247 7.34239 16.3768 7.63901 16.3768ZM10.004 16.3768C10.2926 16.3768 10.5411 16.1084 10.5411 15.783V7.43788C10.5411 7.11254 10.3006 6.84412 10.004 6.84412C9.70738 6.84412 9.46688 7.11254 9.46688 7.43788V15.783C9.46688 16.1084 9.70738 16.3768 10.004 16.3768Z" fill="#C6CDD7"/>
                </svg>
              <% end %>
              <p id="product_price_<%= cart_item.id %>"><%= number_to_currency(cart_item.product.price * cart_item.quantity) %></p>
            </div>
          </div>
        <% end %>
      </div>

      <div class="flex flex-col item-center gap-3 w-full rounded-lg border-2 p-2">
        <div class="flex flex-row justify-between p-2">
          <h2 >Сумма заказа:</h2>
          <p id="cart_total"><%= number_to_currency(@current_cart.total_amount) %></p>
        </div>

        <%= button_to "Оформить", checkout_cart_path, method: :post, class: "rounded-lg py-3 px-5 bg-[#4AD9BD] text-white block font-medium text-center w-full", data: {turbo_frame: "_top"} %>
      </div>
    <% else %>
      <div class="flex items-center justify-center w-full h-full">
